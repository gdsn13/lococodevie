- content_for :head do
  = include_javascripts :image_picker, :edit_page

= f.inputs :name => :information do
  = f.input :title, :wrapper_html => { :class => 'highlighted' }
  = f.input :url
  = f.input :le_bouton
  = f.input :block, :as => :text, :input_html => { :class => 'html' }
  = f.hidden_field :season, :value => @current_site.season_back